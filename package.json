{
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.0",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "semantic-release": "24.2.0"
  },
  "engines": {
    "node": "22"
  },
  "packageManager": "pnpm@9.12.3",
  "private": true,
  "scripts": {
    "build": "pnpm --dir src run build",
    "build:node": "pnpm --dir src run build:node",
    "build:static": "pnpm --dir src run build:static",
    "dev": "pnpm --dir src run dev",
    "generate": "pnpm --dir src run generate",
    "prepare": "husky",
    "start:dev": "pnpm --dir src run start:dev",
    "start:node": "pnpm --dir src run start:node",
    "start:static": "pnpm --dir src run start:static",
    "test:e2e": "pnpm --dir src run test:e2e",
    "test:e2e:docker:br": "pnpm --dir src run test:e2e:docker:br",
    "test:e2e:docker:build": "pnpm --dir src run test:e2e:docker:build",
    "test:e2e:docker:run": "pnpm --dir src run test:e2e:docker:run",
    "test:e2e:docker:server:dev": "pnpm --dir src run test:e2e:docker:server:dev",
    "test:e2e:docker:server:dev:update": "pnpm --dir src run test:e2e:docker:server:dev:update",
    "test:e2e:docker:server:node": "pnpm --dir src run test:e2e:docker:server:node",
    "test:e2e:docker:server:node:update": "pnpm --dir src run test:e2e:docker:server:node:update",
    "test:e2e:docker:server:static": "pnpm --dir src run test:e2e:docker:server:static",
    "test:e2e:docker:server:static:update": "pnpm --dir src run test:e2e:docker:server:static:update",
    "test:e2e:server:dev": "pnpm --dir src run test:e2e:server:dev",
    "test:e2e:server:node": "pnpm --dir src run test:e2e:server:node",
    "test:e2e:server:static": "pnpm --dir src run test:e2e:server:static"
  },
  "type": "module"
}
