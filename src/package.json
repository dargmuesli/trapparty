{
  "author": "Jonas Thelemann",
  "description": "TrapParty's website.",
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "8.4.6",
    "@dargmuesli/nuxt-vio": "13.2.3",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/typescript-urql-graphcache": "3.1.0",
    "@graphql-codegen/urql-introspection": "3.0.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@nuxtjs/i18n": "8.3.0",
    "@pinia-plugin-persistedstate/nuxt": "1.2.1",
    "@pinia/nuxt": "0.5.2",
    "@types/color": "3.0.6",
    "@types/lodash-es": "4.17.12",
    "@urql/core": "5.0.5",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-graphcache": "7.1.2",
    "@urql/vue": "1.4.0",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "chart.js": "4.4.3",
    "color": "4.2.3",
    "consola": "3.2.3",
    "defu": "6.1.4",
    "graphql": "16.9.0",
    "lodash-es": "4.17.21",
    "nuxt": "3.12.4",
    "nuxt-security": "2.0.0-rc.9",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rainbowvis.js": "1.1.1",
    "serve": "14.2.3",
    "sharp": "0.33.4",
    "stylelint": "16.8.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-no-unsupported-browser-features": "8.0.1",
    "tailwindcss": "3.4.7",
    "unhead": "1.9.16",
    "vue": "3.4.35",
    "vue-chartjs": "5.3.1",
    "vue-router": "4.4.2"
  },
  "engines": {
    "node": "20"
  },
  "name": "trapparty",
  "private": true,
  "scripts": {
    "build": "pnpm run build:node",
    "build:node": "nuxt build",
    "build:static": "nuxt generate",
    "dev": "pnpm run start:dev",
    "generate": "pnpm run build:static",
    "gql:codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config .codegenrc.ts",
    "lint": "pnpm run lint:js && pnpm run lint:ts && pnpm run lint:style",
    "lint:fix": "pnpm run lint:js --fix && pnpm run lint:ts --fix && pnpm run lint:style --fix",
    "lint:js": "eslint --cache",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint --cache \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:ts": "nuxt typecheck",
    "prepare": "nuxt prepare && ./.config/certificates/mkcert.sh",
    "preview": "nuxt preview",
    "start:dev": "nuxt dev",
    "start:node": "node .output/server/index.mjs",
    "start:static": "serve .output/public --ssl-cert ./.config/certificates/ssl.crt --ssl-key ./.config/certificates/ssl.key"
  },
  "type": "module",
  "version": "3.15.12"
}
