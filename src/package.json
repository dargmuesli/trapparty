{
  "author": "Jonas Thelemann",
  "description": "TrapParty's website.",
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "9.1.13",
    "@dargmuesli/nuxt-vio": "20.1.5",
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/client-preset": "5.2.2",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-urql-graphcache": "3.1.1",
    "@graphql-codegen/urql-introspection": "3.0.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "@iconify-json/heroicons": "1.2.3",
    "@pinia/nuxt": "0.11.3",
    "@types/color": "4.2.0",
    "@types/lodash-es": "4.17.12",
    "@urql/core": "6.0.1",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-graphcache": "8.1.0",
    "@urql/vue": "2.0.0",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "chart.js": "4.5.1",
    "color": "5.0.3",
    "consola": "3.4.2",
    "defu": "6.1.4",
    "eslint": "9.39.2",
    "graphql": "16.12.0",
    "lodash-es": "4.17.22",
    "nuxt": "4.2.2",
    "nuxt-security": "2.5.1",
    "pinia": "3.0.4",
    "pinia-plugin-persistedstate": "4.7.1",
    "prettier": "3.8.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "rainbowvis.js": "1.1.1",
    "serve": "14.2.5",
    "sharp": "0.34.5",
    "stylelint": "17.0.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "40.0.0",
    "stylelint-no-unsupported-browser-features": "8.0.5",
    "tailwindcss": "4.1.18",
    "unplugin-icons": "23.0.1",
    "unplugin-vue-components": "31.0.0",
    "vue": "3.5.26",
    "vue-chartjs": "5.3.3",
    "vue-router": "4.6.4",
    "vue-tsc": "3.2.2"
  },
  "engines": {
    "node": "24"
  },
  "name": "trapparty",
  "private": true,
  "scripts": {
    "build": "pnpm run build:node",
    "build:node": "nuxt build",
    "build:static": "nuxt generate",
    "dev": "pnpm run start:dev",
    "generate": "pnpm run build:static",
    "gql:codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config .codegenrc.ts",
    "lint": "pnpm run lint:js && pnpm run lint:ts && pnpm run lint:style",
    "lint:fix": "pnpm run lint:js --fix && pnpm run lint:ts --fix && pnpm run lint:style --fix",
    "lint:js": "eslint --cache",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint --cache \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:ts": "nuxt typecheck",
    "prepare": "nuxt prepare && ./.config/certificates/mkcert.sh",
    "preview": "nuxt preview",
    "start:dev": "nuxt dev",
    "start:node": "node .output/server/index.mjs",
    "start:static": "serve .output/public --ssl-cert ./.config/certificates/ssl.crt --ssl-key ./.config/certificates/ssl.key"
  },
  "type": "module",
  "version": "4.3.2"
}
