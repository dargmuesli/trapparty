{
  "author": "Jonas Thelemann",
  "description": "TrapParty's website.",
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "9.0.7",
    "@dargmuesli/nuxt-vio": "18.3.3",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-urql-graphcache": "3.1.1",
    "@graphql-codegen/urql-introspection": "3.0.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "@iconify-json/heroicons": "1.2.2",
    "@pinia-plugin-persistedstate/nuxt": "1.2.1",
    "@pinia/nuxt": "0.11.1",
    "@types/color": "4.2.0",
    "@types/lodash-es": "4.17.12",
    "@urql/core": "5.2.0",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-graphcache": "7.2.4",
    "@urql/vue": "1.4.3",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "chart.js": "4.5.0",
    "color": "5.0.0",
    "consola": "3.4.2",
    "defu": "6.1.4",
    "eslint": "9.31.0",
    "graphql": "16.11.0",
    "lodash-es": "4.17.21",
    "nuxt": "3.17.6",
    "nuxt-security": "2.2.0",
    "pinia": "3.0.3",
    "prettier-plugin-tailwindcss": "0.6.14",
    "rainbowvis.js": "1.1.1",
    "serve": "14.2.4",
    "sharp": "0.34.3",
    "stylelint": "16.21.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "38.0.0",
    "stylelint-no-unsupported-browser-features": "8.0.4",
    "sweetalert2": "11.22.2",
    "tailwindcss": "4.1.11",
    "unplugin-icons": "22.1.0",
    "unplugin-vue-components": "28.8.0",
    "vue": "3.5.17",
    "vue-chartjs": "5.3.2",
    "vue-router": "4.5.1"
  },
  "engines": {
    "node": "22"
  },
  "name": "trapparty",
  "private": true,
  "scripts": {
    "build": "pnpm run build:node",
    "build:node": "nuxt build",
    "build:static": "nuxt generate",
    "dev": "pnpm run start:dev",
    "generate": "pnpm run build:static",
    "gql:codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config .codegenrc.ts",
    "lint": "pnpm run lint:js && pnpm run lint:ts && pnpm run lint:style",
    "lint:fix": "pnpm run lint:js --fix && pnpm run lint:ts --fix && pnpm run lint:style --fix",
    "lint:js": "eslint --cache",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint --cache \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:ts": "nuxt typecheck",
    "prepare": "nuxt prepare && ./.config/certificates/mkcert.sh",
    "preview": "nuxt preview",
    "start:dev": "nuxt dev",
    "start:node": "node .output/server/index.mjs",
    "start:static": "serve .output/public --ssl-cert ./.config/certificates/ssl.crt --ssl-key ./.config/certificates/ssl.key"
  },
  "type": "module",
  "version": "4.1.13"
}
